{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
  "comma-dangle": ["error", "always-multiline"],
  "linebreak-style": ["error", "unix"],
  "no-tabs": "error",
  "semi": ["error", "never"],
  "sort-imports": "error"
  }
}
