{
  "name": "bs-custom-file-input",
  "version": "1.0.0",
  "description": "A little plugin for Bootstrap 4 custom file input",
  "main": "dist/bs-custom-file-input.js",
  "bugs": {
    "url": "https://github.com/Johann-S/bs-custom-file-input/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "rollup --environment TEST -c -w",
    "build": "rollup -c && rollup -c --environment PROD",
    "pretest": "cross-env NODE_ENV=test rollup -c --environment TEST",
    "test": "npm run js-lint && npm run js-test && npm run coveralls",
    "js-lint": "eslint ./src ./tests",
    "js-test": "karma start karma.conf.js",
    "coveralls": "shx cat tests/coverage/lcov.info | coveralls",
    "browserstack": "cross-env browser=true karma start karma.conf.js"
  },
  "files": [
    "dist/*.{js,map}"
  ],
  "keywords": [
    "bootstrap",
    "bootstrap 4",
    "custom file input"
  ],
  "author": "Johann-S <johann.servoire@gmail.com>",
  "homepage": "https://github.com/Johann-S/bs-custom-file-input",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Johann-S/bs-custom-file-input.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-plugin-istanbul": "^5.0.1",
    "bootstrap": "^4.1.3",
    "chai": "^4.1.2",
    "core-js": "^2.5.7",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "karma": "^3.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "mocha": "^5.2.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-uglify": "^4.0.0",
    "shx": "^0.3.2",
    "sinon": "^6.1.5"
  },
  "dependencies": {}
}
